{
  "hosting": {
    "headers": [
      {
        "source" : "index.html",
        "headers" : [ {
          "key" : "Access-Control-Allow-Origin",
          "value" : "*"
        } ]
      },
      { "source":"/service-worker.js", "headers": [{"key": "Cache-Control", "value": "no-cache"}] },
      { "source":"/workbox-e170c028.js", "headers": [{"key": "Cache-Control", "value": "no-cache"}] },
      { "source":"/firebase-messaging-sw.js", "headers": [{"key": "Cache-Control", "value": "no-cache"}] },
      { "source":"/manifest.json", "headers": [{"key": "Cache-Control", "value": "no-cache"}] }
    ],
    "target": "client",
    "public": "dist/pwa",
    "rewrites": [
      {
        "source": "/getmanifest.json",
        "function": "GetManifest"
      },
      {
        "source": "/transact",
        "function": "MakePay"
      },
      {
        "source": "/getauth",
        "function": "GetAuth"
      },
      {
        "source": "/getinitjs",
        "function": "getinitjs"
      },
      {
        "source": "/getmanifestadm.json",
        "function": "GetManifestAdmin"
      }
    ]
  }
}
