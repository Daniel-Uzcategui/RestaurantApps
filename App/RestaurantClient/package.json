{
  "name": "chopzi",
  "version": "1.0.1",
  "description": "Chopzi ECRA",
  "productName": "Chopzi",
  "author": "Pfcevolution",
  "private": true,
  "scripts": {
    "dev": "cross-env QENV=development quasar dev",
    "dev-prod": "cross-env QENV=dev-prod quasar dev",
    "dev-chopzi": "cross-env QENV=chopzi quasar dev",
    "prod": "cross-env QENV=prod quasar dev",
    "dev-bar": "cross-env QENV=bar quasar dev",
    "dev-pwa": "cross-env QENV=development quasar dev -m pwa",
    "dev-qa": "cross-env QENV=qa quasar dev",
    "lint": "eslint --ext .js,.vue src",
    "test": "echo \"No test specified\" && exit 0",
    "build": "cross-env QENV=development quasar build",
    "build-dev": "cross-env QENV=development quasar build -m pwa",
    "build-prod": "cross-env QENV=prod quasar build -m pwa",
    "build-prod-spa": "cross-env QENV=prod quasar build -m spa",
    "build-qa": "cross-env QENV=qa quasar build",
    "build-ecr": "cross-env QENV=ecr quasar build",
    "deploy-dev": "firebase use default && yarn build-dev && node addmanifest.js deploy -d && firebase deploy",
    "deploy-prod": "firebase use prodchopzi && yarn build-prod && node addmanifest.js deploy -p",
    "deploy-prod-spa": "firebase use prodchopzi && yarn build-prod-spa && node addmanifest.js deploy -a",
    "deploy-dev2": "firebase use default && yarn build-dev && firebase deploy"
  },
  "dependencies": {
    "@firebase/storage": "^0.4.0",
    "@quasar/extras": "^1.15.3",
    "atob": "^2.1.2",
    "axios": "^0.19.2",
    "browser-image-compression": "^1.0.14",
    "browserslist": "^4.20.3",
    "core-js": "3.0.0",
    "draggable-vue-directive": "^2.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "firebase": "9.14.0",
    "fuse.js": "^6.6.2",
    "gmap-vue": "^1.2.1",
    "quasar": "^1.20.1",
    "quasar-app-extension-firebase-messaging-fb9": "^0.1.2",
    "vue-carousel": "^0.18.0",
    "vue-croppa": "^1.3.8",
    "vue-paycard": "^0.3.0",
    "vue-router": "^3.5.4",
    "vue-typer": "^1.2.0",
    "vuex-persistedstate": "^4.0.0-beta.1",
    "vuexfire": "^3.2.4"
  },
  "devDependencies": {
    "@quasar/app": "2.4.2",
    "@quasar/quasar-app-extension-qenv": "^1.1.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "cheerio": "^1.0.0-rc.5",
    "cross-env": "^7.0.3",
    "eslint": "^5.10.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "firebase-admin": "^9.6.0",
    "fs-extra": "^9.1.0",
    "pug": "^3.0.2",
    "pug-plain-loader": "^1.1.0",
    "vue-template-compiler": "2.6.12",
    "workbox-webpack-plugin": "5.0.0",
    "yargs": "^16.2.0"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
    "cover 95%"
  ]
}
